{% extends 'AppBundle:look4wear:layout.html.twig' %}

{% block content %}
    <div class="goodsBlock">
        {{ include('AppBundle:look4wear:goods.block.html.twig', { 'goods': goods }) }}
    </div>
    {% if category.description %}
        <div style="clear: both;"></div>
        {{ category.description|raw }}
    {% endif %}
    {% if categoryTopVendorsResult %}
        {{ include('AppBundle:look4wear:top.vendors.block.html.twig', { 'categoryTopVendorsResult': categoryTopVendorsResult, 'title': 'Популярные бренды этой категории' }) }}
    {% endif %}
{% endblock %}
